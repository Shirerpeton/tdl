{
  "name": "tdl",
  "version": "1.0.0",
  "description": "Simple To Do List",
  "main": "app.js",
  "scripts": {
    "server": "nodemon app.js",
    "client": "npm start --prefix tdl-client",
    "db": "nodemon db.js",
    "start": "concurrently \"npm run server\" \"npm run client \" -k",
    "test": "nyc mocha --timeout 10000",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov -t 2d0d63ed-db5b-4d3a-bcd8-77f46f48f982"
  },
  "author": "HyloxalusFallax",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "amqplib": "^0.5.6",
    "amqplib-rpc": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "codecov": "^3.6.5",
    "concurrently": "^5.1.0",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.3.0",
    "koa-generic-session": "^2.0.4",
    "koa-redis": "^4.0.1",
    "koa-router": "^8.0.8",
    "mocha": "^10.2.0",
    "moment": "^2.24.0",
    "nodemon": "^2.0.3",
    "nyc": "^15.0.1",
    "pg": "^8.1.0",
    "sinon": "^9.0.2",
    "socket.io": "^2.3.0",
    "socket.io-cookie": "0.0.1",
    "yup": "^0.28.3"
  }
